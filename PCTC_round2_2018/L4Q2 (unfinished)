d=int(input())
n=int(input())
corrdinates=[]
for i in range(n):
    x, y=input().split()
    x=int(x)
    y=int(y)
    coor[i].append(x, y)
    
fires=0
curr_coor=(0, 0)
lit
for i in range(n):
